<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="navvis">
	<link name="base">
		<visual>
			<!-- Shift the box up half its height. !-->
			<origin xyz="0.0 0.0 0.96" rpy="0.0 0.0 0.0" />
			<geometry>
				<box size="0.45 0.78 1.92" />
			</geometry>
		</visual>
		<collision>
			<!-- Shift the box up half its height. !-->
			<origin xyz="0.0 0.0 0.96" rpy="0.0 0.0 0.0" />
			<geometry>
				<box size="0.45 0.78 1.92" />
			</geometry>
		</collision>
	</link>
	<link name="laser_vert_bottom">
		<visual>
			<!-- Shift the box up half its height. !-->
			<origin xyz="0.0 0.0 0.0435" rpy="0.0 0.0 0.0" />
			<geometry>
				<box size="0.062 0.062 0.087" />
			</geometry>
		</visual>
		<collision>
			<!-- Shift the box up half its height. !-->
			<origin xyz="0.0 0.0 0.0435" rpy="0.0 0.0 0.0" />
			<geometry>
				<box size="0.062 0.062 0.0435" />
			</geometry>
		</collision>
	</link>
	<joint name="base_laser_vert_bottom_joint" type="fixed">
		<parent link="base" />
		<child link="laser_vert_bottom" />
		<origin xyz="0.3158 -0.0021 0.5100" rpy="3.1355 -1.3045 0.0025" />
	</joint>
	<link name="laser_vert_top_right">
		<visual>
			<!-- Shift the box up half its height. !-->
			<origin xyz="0.0 0.0 0.0435" rpy="0.0 0.0 0.0" />
			<geometry>
				<box size="0.062 0.062 0.087" />
			</geometry>
		</visual>
		<collision>
			<!-- Shift the box up half its height. !-->
			<origin xyz="0.0 0.0 0.0435" rpy="0.0 0.0 0.0" />
			<geometry>
				<box size="0.062 0.062 0.0435" />
			</geometry>
		</collision>
	</link>
	<joint name="base_laser_vert_top_right_joint" type="fixed">
		<parent link="base" />
		<child link="laser_vert_top_right" />
		<origin xyz="0.2978 -0.1450 1.8451" rpy="-1.1760 0.6313 -1.9377" />
	</joint>
	<link name="laser_vert_top_left">
		<visual>
			<!-- Shift the box up half its height. !-->
			<origin xyz="0.0 0.0 0.0435" rpy="0.0 0.0 0.0" />
			<geometry>
				<box size="0.062 0.062 0.087" />
			</geometry>
		</visual>
		<collision>
			<!-- Shift the box up half its height. !-->
			<origin xyz="0.0 0.0 0.0435" rpy="0.0 0.0 0.0" />
			<geometry>
				<box size="0.062 0.062 0.0435" />
			</geometry>
		</collision>
	</link>
	<joint name="base_laser_vert_top_left_joint" type="fixed">
		<parent link="base" />
		<child link="laser_vert_top_left" />
		<origin xyz="0.2997 0.1531 1.8443" rpy="1.1780 0.6198 1.9560" />
	</joint>
	<link name="laser_horiz">
		<visual>
			<!-- Shift the box up half its height. !-->
			<origin xyz="0.0 0.0 0.026" rpy="0.0 0.0 0.0" />
			<geometry>
				<cylinder radius="0.052" length="0.072" />
			</geometry>
		</visual>
		<collision>
			<!-- Shift the box up half its height. !-->
			<origin xyz="0.0 0.0 0.026" rpy="0.0 0.0 0.0" />
			<geometry>
				<cylinder radius="0.052" length="0.072" />
			</geometry>
		</collision>
	</link>
	<joint name="base_laser_horiz" type="fixed">
		<parent link="base" />
		<child link="laser_horiz" />
		<origin xyz="0.2325 0.001 1.9201" rpy="-0.0012 0.1146 -0.0015" />
	</joint>
	<link name="wheel_center">
		<visual>
			<!-- Shift the box up half its height. !-->
			<origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
			<geometry>
				<cylinder radius="0.15" length="0.05" />
			</geometry>
		</visual>
		<collision>
			<!-- Shift the box up half its height. !-->
			<origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
			<geometry>
				<cylinder radius="0.15" length="0.05" />
			</geometry>
		</collision>
	</link>
	<joint name="base_wheel_center" type="continuous">
		<parent link="base" />
		<child link="wheel_center" />
		<axis xyz="0.0 0.0 1.0"/>
		<origin xyz="0.10 0.0 0.0" rpy="-1.57 0.0 0.0" />
		<limit effort="20.0" velocity="1.0" lower="-3.14" upper="3.14" />
	</joint>
	<!-- XACRO Macro definition block to add around the wheel link and joint !-->
	<xacro:macro name="xacro_name" params="side reflect">
		<link name="wheel_${side}">
			<visual>
				<!-- Shift the box up half its height. !-->
				<origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
				<geometry>
					<cylinder radius="0.2" length="0.05" />
				</geometry>
			</visual>
			<collision>
				<!-- Shift the box up half its height. !-->
				<origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
				<geometry>
					<cylinder radius="0.2" length="0.05" />
				</geometry>
			</collision>
		</link>
		<joint name="base_wheel_${side}" type="continuous">
			<parent link="base" />
			<child link="wheel_${side}" />
			<axis xyz="0.0 0.0 1.0"/>
			<origin xyz="-0.10 ${reflect * 0.4} 0.0" rpy="-1.57 0.0 0.0" />
			<limit effort="20.0" velocity="1.0" lower="-3.14" upper="3.14" />
		</joint>
	</xacro:macro>
	<!-- Call the XACRO Macro. It should be the left wheel !-->
	<xacro:xacro_name side="left" reflect="1" />
	<xacro:xacro_name side="right" reflect="-1" />
</robot>
